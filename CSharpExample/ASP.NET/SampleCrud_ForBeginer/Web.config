<configuration>
  <!--
    有关 web.config 更改的说明，请参见 http://go.microsoft.com/fwlink/?LinkId=235367。

    可在 <httpRuntime> 标记上设置以下特性。
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.6.1"/>
    <httpRuntime targetFramework="4.6"/>
    <!--老版本配置-->
    <!--<httpHandlers>
      <add verb="*" path="proList.aspx" type="HW.ProductListHandler" />
      <add verb="*" path="AddPro.aspx" type="HW.AddProductHandler" />
      <add verb="*" path="ToList.aspx" type="HW.DeleteProductHandler" />
      <add verb="*" path="Update.aspx" type="HW.UpdateProductHandler" />
    </httpHandlers>-->
  </system.web>
  <system.webServer>
    <defaultDocument>
      <files>
        <add value="Template/AddProducts.html"/>
      </files>
    </defaultDocument>
    <!--新配置-->
    <handlers>
      <add verb="*" path="proList.aspx" type="HW.ProductListHandler" name="ProductListHandler"/>
      <add verb="*" path="AddPro.aspx" type="HW.AddProductHandler" name="ProductAddHandler"/>
      <add verb="*" path="ToList.aspx" type="HW.DeleteProductHandler" name="ProductDeleteHandler"/>
      <add verb="*" path="Update.aspx" type="HW.UpdateProductHandler" name="ProductUpdateHandler"/>
    </handlers>
  </system.webServer>
</configuration>