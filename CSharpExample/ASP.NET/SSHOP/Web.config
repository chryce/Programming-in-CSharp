<?xml version="1.0"?>
<!--
  有关如何配置 ASP.NET 应用程序的详细消息，请访问
  http://go.microsoft.com/fwlink/?LinkId=169433
  -->
<configuration>
  <!--
    有关 web.config 更改的说明，请参见 http://go.microsoft.com/fwlink/?LinkId=235367。

    可在 <httpRuntime> 标记上设置以下特性。
      <system.Web>
        <httpRuntime targetFramework="4.6.1" />
      </system.Web>
  -->
  <system.web>
    <compilation debug="true" targetFramework="4.6.1"/>
    <httpHandlers>
      <add verb="*" path="Login.html" type="SSHOP.LoginHandler"/>
      <add verb="*" path="Register.html" type="SSHOP.RegisterHandler"/>
      <add verb="*" path="GoodsDetails.html" type="SSHOP.DisplayGoodsHandler"/>
      <add verb="*" path="BuyProduct.html" type="SSHOP.DetailGoodsHandler"/>
      <add verb="*" path="SubOrder.html" type="SSHOP.OrederIISHandler"/>
    </httpHandlers>
    <pages controlRenderingCompatibilityVersion="4.0"/>
  </system.web>
</configuration>